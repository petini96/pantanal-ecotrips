{
    "name": "catalogo-bolerada",
    "version": "1.0.0",
    "description": "A Quasar Project",
    "productName": "Artesanal Doceria",
    "author": "Vin√≠cius <petiniprojetos96@gmail.com>",
    "type": "module",
    "scripts": {
        "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
        "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
        "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
        "serve": "quasar serve",
        "dev": "quasar dev",
        "dev:ssr": "quasar dev -m ssr",
        "build:ssr": "quasar build -m ssr",
        "start:ssr": "node dist/ssr/index.js",
        "build": "quasar build",
        "postinstall": "quasar prepare",
        "build:pwa": "npm run generate:sitemap && quasar build -m pwa",
        "dev:vite": "vite",
        "build:vite": "vue-tsc -b && vite build",
        "preview:vite": "vite preview",
        "serve:vite": "vite preview",
        "preview": "npx vite preview --base=/ --port 5173 --host --outDir dist/spa",
        "test:e2e": "cross-env NODE_ENV=test TS_NODE_PROJECT=test/cypress/tsconfig.json start-test \"quasar dev\" http-get://127.0.0.1:9000 \"cypress open --e2e\"",
        "test:e2e:open": "cypress open",
        "test:e2e:ci": "cross-env NODE_ENV=test TS_NODE_PROJECT=test/cypress/tsconfig.json start-test \"quasar dev\" http-get://127.0.0.1:9000 \"cypress run --e2e\"",
        "test:component": "cross-env NODE_ENV=test TS_NODE_PROJECT=test/cypress/tsconfig.json cypress open --component",
        "test:component:ci": "cross-env NODE_ENV=test TS_NODE_PROJECT=test/cypress/tsconfig.json cypress run --component",
        "generate:sitemap": "cross-env TS_NODE_PROJECT=./scripts/tsconfig.json node --loader ts-node/esm ./scripts/generate-sitemap.ts"
    },
    "dependencies": {
        "@electron/remote": "^2.1.2",
        "@fortawesome/fontawesome-svg-core": "^6.2.0",
        "@fortawesome/free-brands-svg-icons": "^6.2.0",
        "@fortawesome/free-regular-svg-icons": "^6.2.0",
        "@fortawesome/free-solid-svg-icons": "^6.2.0",
        "@fortawesome/vue-fontawesome": "^3.0.1",
        "@popperjs/core": "^2.11.6",
        "@quasar/extras": "^1.16.11",
        "@typebot.io/js": "^0.3.78",
        "axios": "^1.7.7",
        "catalogo-bolerada": "file:",
        "compression": "^1.8.1",
        "express": "^5.1.0",
        "keycloak-js": "^26.2.0",
        "pinia": "^2.3.1",
        "quasar": "^2.18.5",
        "quasar-project": "file:",
        "serialize-javascript": "^7.0.0",
        "vue": "^3.4.18",
        "vue-gtag": "^3.0.0",
        "vue-i18n": "^11.1.11",
        "vue-router": "^4.0.12",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@quasar/app-vite": "^2.4.0",
        "@quasar/quasar-app-extension-testing-e2e-cypress": "^6.2.1",
        "@types/node": "^20.5.9",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vue/eslint-config-prettier": "^10.1.0",
        "@vue/eslint-config-typescript": "^14.1.3",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.2",
        "cross-env": "^10.0.0",
        "cypress": "^14.2.1",
        "eslint": "^9.14.0",
        "eslint-plugin-cypress": "^4.2.1",
        "eslint-plugin-vue": "^9.30.0",
        "globals": "^15.12.0",
        "prettier": "^3.3.3",
        "rollup-plugin-visualizer": "^6.0.5",
        "sass-embedded": "^1.83.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "vite": "^6.0.7",
        "vite-plugin-checker": "^0.8.0",
        "vue-tsc": "^2.0.29",
        "xmlbuilder2": "^3.1.1"
    },
    "engines": {
        "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
        "npm": ">= 6.13.4",
        "yarn": ">= 1.21.1"
    }
}
